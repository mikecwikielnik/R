# ---Variables---
x <- 2                               # create variable x with the value 2
y <- 3                               # create variable y with the value 3
xsum<-(x + y)                        # create variable xsum with the value of x+y
xmult<-(x * y)                       # create variable xmult with the value of x*y
print(x)                             # print contents of variable x
print(y)                             # print contents of variable y
print(xsum)                          # print contents of variable xsum
print(xmult)                         # print contents of variable xmult
